<!DOCTYPE html>
<html>

<head>
  <title>EJS compilation demo</title>
  <style></style>
</head>

<body>
  <% function userView(user, i) { %>
    <tr>
      <td>
        <%= i %>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= user.age %>
      </td>
      <td>
        <%= user.species %>
      </td>
    </tr>
    <% } %>
      <table border="1">
        <tr>
          <td>#</td>
          <td>Name</td>
          <td>Age</td>
          <td>Species</td>
        </tr>
        <% users.map(userView) %>
      </table>
      <p>
        <a href="/setcookie">set cookie</a>
      </p>
      <p>
        <a href="/clearcookie">clear cookie</a>
      </p>

</body>

</html>